{
    "version": 8,
    "configurePresets": [
        {
            "name": "def",
            "displayName": "GCC 13.3.0 x86_64-linux-gnu",
            "description": "Using compilers: C = /usr/bin/gcc-13, CXX = /usr/bin/g++-13",
            "binaryDir": "${sourceDir}/build/",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/out/install/${presetName}",
                "CMAKE_C_COMPILER": "/usr/bin/gcc-13",
                "CMAKE_CXX_COMPILER": "/usr/bin/g++-13",
                "CMAKE_BUILD_TYPE": "Debug",
                "BUILD_WORKER_REAL": "ON",
                "BUILD_POSTPROCESSOR_REAL": "ON"
            }
        },
        {
          "name": "defWin",
          "displayName": "w64devkit GCC (local in /ext)",
          "description": "Use w64devkit bundled in project's ext/ folder",
          "generator": "MinGW Makefiles",
          "binaryDir": "${sourceDir}/build/",
          "cacheVariables": {
            "CMAKE_INSTALL_PREFIX": "${sourceDir}/out/install/${presetName}",
            "CMAKE_C_COMPILER": "${sourceDir}/ext/w64devkit-1.16.1/w64devkit/bin/gcc.exe",
            "CMAKE_CXX_COMPILER": "${sourceDir}/ext/w64devkit-1.16.1/w64devkit/bin/g++.exe",
            "CMAKE_MAKE_PROGRAM": "${sourceDir}/ext/w64devkit-1.16.1/w64devkit/bin/mingw32-make.exe",
            "CMAKE_BUILD_TYPE": "Debug",
            "BUILD_WORKER_REAL": "ON",
            "BUILD_POSTPROCESSOR_REAL": "ON"
          }
        }
    ],
    "buildPresets": [
        {
            "name": "def",
            "description": "",
            "displayName": "",
            "configurePreset": "def",
            "jobs": 15
        },
        {
            "name": "defWin",
            "description": "",
            "displayName": "",
            "configurePreset": "defWin",
            "jobs": 15
        }
    ]
}